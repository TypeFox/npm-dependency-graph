{
  "name": "depgraph-navigator",
  "version": "0.0.0",
  "description": "A Theia extension that visualizes npm package dependencies",
  "license": "Apache-2.0",
  "keywords": [
    "theia-extension", "npm", "dependencies", "graph"
  ],
  "homepage": "http://npm-dependencies.com/",
  "bugs": "https://github.com/TypeFox/npm-dependency-graph/issues",
  "author": {
    "name": "TypeFox"
  },
  "contributors": [
    {
      "name": "Miro Sp√∂nemann",
      "email": "miro.spoenemann@typefox.io",
      "url": "https://typefox.io"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeFox/npm-dependency-graph.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "latest",
    "@theia/navigator": "latest",
    "elkjs": "latest",
    "semver": "^5.5.0",
    "sprotty": "latest",
    "theia-sprotty": "latest"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/frontend-extension",
      "backend": "lib/server/backend-extension"
    }
  ]
}

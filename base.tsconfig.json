{
    "compilerOptions": {
        "strict": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "typeRoots": [
            "typings"
        ],
        "sourceMap": true,
        "declaration": true
    }
}